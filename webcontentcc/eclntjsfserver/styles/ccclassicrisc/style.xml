    <tag name="t:tabbedlinecontainer" variant="cctl01" >
      <set attribute="bgpaintdefault" value="rectangle(0,0,100%,100%,#d0d0d0,#f0f0f0,vertical)" />
      <set attribute="bgpaintdefaultfirst" value="roundedrectangle(0,0,50,100%,25,25,#d0d0d0,#f0f0f0,vertical);rectangle(25,0,100%,100%,#d0d0d0,#f0f0f0,vertical)" />
      <set attribute="bgpaintdefaultlast" value="roundedrectangle(0,0,100%,100%,25,25,#d0d0d0,#f0f0f0,vertical);rectangle(0,0,100%-25,100%,#d0d0d0,#f0f0f0,vertical)" />
      <set attribute="bgpaintdefaultonly" value="roundedrectangle(0,0,100%,100%,25,25,#d0d0d0,#f0f0f0,vertical)" />
      <set attribute="bgpaintrollover" value="rectangle(0,0,100%,100%,#e9b475,#fefdd3,vertical)" />
      <set attribute="bgpaintrolloverfirst" value="roundedrectangle(0,0,50,100%,25,25,#e9b475,#fefdd3,vertical);rectangle(25,0,100%,100%,#e9b475,#fefdd3,vertical)" />
      <set attribute="bgpaintrolloverlast" value="roundedrectangle(0,0,100%,100%,25,25,#e9b475,#fefdd3,vertical);rectangle(0,0,100%-25,100%,#e9b475,#fefdd3,vertical)" />
      <set attribute="bgpaintrolloveronly" value="roundedrectangle(0,0,100%,100%,25,25,#e9b475,#fefdd3,vertical)" />
      <set attribute="bgpaintselected" value="rectangle(0,0,100%,100%,#75b4e9,#d3fdfe,vertical)" />
      <set attribute="bgpaintselectedfirst" value="roundedrectangle(0,0,50,100%,25,25,#75b4e9,#d3fdfe,vertical);rectangle(25,0,100%,100%,#75b4e9,#d3fdfe,vertical)" />
      <set attribute="bgpaintselectedlast" value="roundedrectangle(0,0,100%,100%,25,25,#75b4e9,#d3fdfe,vertical);rectangle(0,0,100%-25,100%,#75b4e9,#d3fdfe,vertical)" />
      <set attribute="bgpaintselectedonly" value="roundedrectangle(0,0,100%,100%,25,25,#75b4e9,#d3fdfe,vertical)" />
      <set attribute="fontselected" value="weight:bold" />
      <set attribute="foregrounddefault" value="#404040" />
